/* 初始化CSS */
*{
	margin:0;
	padding:0;
}
html,body,#root{
	overflow:hidden;
	width: 100%;
	height: 100%
}
body, dl, dd, h1, h2, h3, h4, h5, h6, p, form{
	margin: 0;
}
body {
	background: #fff;
	color: #333;
	font: 12px/1.5 "微软雅黑", Arial, sans-serif;
}
img,button {
	border: 0;
}
ul, ol {
	list-style-type: none;
	margin: 0;
	padding: 0;
}
input, img, select {
	vertical-align: middle;
}
a {
	text-decoration: none;
}
a{
	color: #333;
}
a:hover {
	color: #c00;
}
em, i {
	font-style: normal;
}
/* 全局样式 */
.fix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
	font-size: 0;
}
.fix {
*zoom:1;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.btn-blue{
	padding:10px 30px;
	font-size:16px;
	color:#fff;
	background: #12aaeb;
	display:inline-block;
	border-radius:40px;
}
.editor-content{
	clear: both;
	position: relative;
	height:100%;
	margin-top:66px;
}
#pageTree{
	float: left;
	width: 150px;
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	background:#eee;
}

#pageTree li{

	width: 85%;
	height: 140px;
	background: #fff;
	border: 1px solid #ddd;
	margin: 10px 10px 15px;
	cursor: pointer;
	position: relative;

}

.up{
	background: url('../img/up.png');
	width: 13px;
	height: 20px;
	position: absolute;
	left: 9px;
	top: 10px
}

.down{
	background: url('../img/down.png');
	width: 13px;
	height: 20px;
	position: absolute;
    left: 9px;
	bottom:5px
}
.pageNum{
	font-size: 20px;
	position: absolute;
	left: 0px;
	top: 46%;
	color: #666;
	display: block;
	width: 100%;
	text-align: center;
}
.close{
	background: url('../img/close.png') #fff;
	border-radius: 50%;
	background-size:100%;
	width: 20px;
	height: 20px;
	position: absolute;
	right: -10px;
	top: -10px;
	opacity: 0

}
#pageTree ul{
	height: 95%;
	overflow-y: auto;
}
#pageTree li:hover {
	background: #333
}


#pageTree li:hover .close{
	opacity: 1
}

#pageTree li:active,#pageTree li.active{
	background-color: #12aaeb;
	border: 1px dotted #ccc;
	box-shadow: 0 0 5px #12aaeb
}

#pageTree .inner{
	background: #fff;
	width: 100%;
	height: 100%
}

#pageTree #add{
	position: absolute;
	text-align: center;
	width: 100%;
	padding: 0 5px 5px;
	bottom: 0
}

#sidebar{

	float: right;
	width: 350px;
	position: absolute;
	right: 0;
	top: 0;
	height: 100%;
	overflow-y:auto;
	background:#fff;
	padding: 0 30px;



}

#editorContent{
	padding: 0 410px 0 150px;
		height: 100%;
		background: #444;
		overflow: hidden;

}



.m-editor{
	width: 384px;
	height: 624px;
	background: #fff;
	border: 1px solid #ddd;
	margin: 20px auto 0;
	position: relative;
	box-shadow: 0 0 4px 1px green,0 1px 10px 1px #2d95f0;
	background-size: cover;
	background-position: center center
}

.elements{
	position:absolute;
	display: inline-block;
	cursor:pointer; 
}

.singleimg{
	display: block;
	width: 100%;
}

.selected{
	box-shadow: 0 0 6px #f00;

}


#toolbar{
	margin: 10px;
	text-emphasis: close;
	text-align: center;
}

.effect-list li{
    border: 1px solid #d9d9d9;
    float: left;
    font-size: 12px;
    height: 48px;
    line-height: 50px;
    margin: 0 0 30px 27px;
    text-align: center;
    width: 49px;
    cursor: pointer;
}
.effect-list li:hover i, .effect-list li.on i{
	width: 11px;
	height: 11px;
	display:block;
	position: absolute;
	margin:0 0 0 38px;
	background: url(../img/icon-r.jpg);
}
.effect-list li:hover, .effect-list li.on{
	border: 1px solid #12aaeb;
}
.effect-list li div{
	position: absolute;
    margin: 50px 0 0 0;
    width: 48px;
    height: 20px;
    line-height: 20px;
	color:#acacac;
}
.effect-list li:hover div,.effect-list li.on div{
	color:#12aaeb;
}

.u-Layer-ctrl{
    position: absolute;
    width: 100px;
    height: 200px;
    background: #fff;
    padding: 10px;
    box-shadow: 1px 2px 3px #333;
    z-index: 999
}

.u-Layer-ctrl li {
    line-height: 1.7;
    cursor:pointer;
    padding: 5px;
}
.u-Layer-ctrl li:hover{
    background: #eee
}
/*uploader */

#insertImage {
    background: white none repeat scroll 0 0;
    border: 2px dashed #0087f7;
    border-radius: 5px;
    width: 800px;
    position: absolute;
    left: 0;
    top: 200px;
    right: 0;
    margin: 0 auto;
    box-shadow: 1px 1px 3px #ddd;
    max-height: 500px;
    overflow-y: auto;
}

#insertImage img{
    border-radius: 5px
}

#insertImage .action{
}

