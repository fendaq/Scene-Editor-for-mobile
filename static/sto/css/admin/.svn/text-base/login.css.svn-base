body,img,p,div,ul,li,a,form,input,select{margin:0px; padding:0px;}
body{ background:#D2DCE5; font-size:14px;}
.box{ height:300px; width:590px; margin:85px auto 0px auto; border:1px solid #7991A7; background:#fff; position:relative;}
.box-left{ width:160px; height:300px; float:left;}
.box-right{ float:right; width:430px;}
.clear{ clear:both; height:0px; overflow:hidden;}
ul,li{ list-style:none;}
.username,.password{ float:left; border:1px solid #ccc; width:150px; height:23px; line-height:23px; padding-left:20px; font-size:14px;}
.code{ float:left; border:1px solid #ccc; width:80px; height:23px; line-height:23px;  font-size:14px;}
.username{ background:url(../../img/user.gif) no-repeat 2px 4px;}
.password{ background:url(../../img/key.gif) no-repeat 2px 4px;}
.userlogin li span{ float:left; margin-right:24px;}
li .error{color: red;font-size:12px;}
.userlogin li{ height:23px; clear:both; line-height:23px; margin-bottom:25px;}
.userlogin{ margin:26px 0px 0px 66px;}
.userbtn{ text-align:center;}
.userbtn input{ border:1px solid #ccc; color:#666; background:#eee; width:68px; height:23px; cursor:pointer; margin:0px 20px;}
.box-bg{ position:relative; background:#939EA9; margin:0px auto; left:5px; height:300px; width:590px; top:-295px; z-index:-1;}
.warning{ font-size:12px; margin-left:100px; background:url(../../img/x.gif) no-repeat 3px 2px; padding-left:30px; line-height:20px; height:20px; visibility:hidden;}