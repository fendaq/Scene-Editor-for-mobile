/* 
 * My97 DatePicker 4.8
 */

.WdateDiv{
	width:275px;
	background-color:#FFFFFF;
	border:#dbddd9 1px solid;
	border-radius:3px;  
}

.WdateDiv2{
	width:360px;
}
.WdateDiv *{
  font-size:14px;
  font-family:'����';
  color:#626262;
 }

.WdateDiv .NavImg a{
	display:block;
	cursor:pointer;
	height:24px;
	width:24px;
	margin-top:6px;
}

.WdateDiv .NavImgll a{
	float:left;
	background:transparent url(img.gif) no-repeat scroll 0 0;
	display:none;
}
.WdateDiv .NavImgl a{
	float:left;
	background:transparent url(date.png) no-repeat scroll 0 -48px;
	margin-left:10px;
	margin-right:50px;
}
.WdateDiv .NavImgl a:hover{background-position:0 -72px}
.WdateDiv .NavImgr a{
	float:right;
	background:transparent url(date.png) no-repeat scroll 0 0;
	margin-right:10px;
}
.WdateDiv .NavImgr a:hover{background-position:0 -24px}
.WdateDiv .NavImgrr a{
	float:right;
	background:transparent url(img.gif) no-repeat scroll -48px 0;
	display:none;
}

.WdateDiv #dpTitle{
	height:36px;
	line-height:36px;
	margin-bottom:10px;
	background:#f3f3f3;
}

.WdateDiv .yminput{
	margin-top:8px;
	text-align:center;
	height:20px;
	border:0px;
	width:50px;
	cursor:pointer;	
    background:#f3f3f3;	
}

.WdateDiv .yminputfocus{
	margin-top:8px;
	text-align:center;
	font-weight:bold;
	height:20px;
	color:blue;
	border:#ccc 1px solid;
	width:50px;
}

.WdateDiv .menuSel{
	z-index:1;
	position:absolute;
	background-color:#FFFFFF;	
	border:#ccc 1px solid;
	display:none;
}

.WdateDiv .menu{
	cursor:pointer;
	background-color:#fff;
}

.WdateDiv .menuOn{
	cursor:pointer;
	background-color:#BEEBEE;
}

.WdateDiv .invalidMenu{
	color:#aaa;
}

.WdateDiv .YMenu{
	margin-top:28px;
	
}

.WdateDiv .MMenu{
	margin-top:28px;
	*width:62px;
}

.WdateDiv .hhMenu{
	margin-top:-90px; 
	margin-left:26px;
}

.WdateDiv .mmMenu{
	margin-top:-46px; 
	margin-left:26px;
}

.WdateDiv .ssMenu{
	margin-top:-24px; 
	margin-left:26px;
}

 .WdateDiv .Wweek {
 	text-align:center;
	background:#DAF3F5;
	border-right:#BDEBEE 1px solid;
 }

.WdateDiv .MTitle{
	background-color:#e8f3fa;
	line-height:36px;
}
.WdateDiv .WdayTable2{
	border-collapse:collapse;
	border:#c5d9e8 1px solid;
}
.WdateDiv .WdayTable2 table{
	border:0;
}

.WdateDiv .WdayTable{
	border-bottom:#d5d3d4 1px solid;
	padding-bottom:10px;
	
}
.WdateDiv .WdayTable td{
	text-align:center;
	vertical-align:middle;
	line-height:14px;
	padding:7px 0;
}
.WdateDiv .Wday{
	cursor:pointer;
}

.WdateDiv .WdayOn{
	cursor:pointer;
	background:#6cbcf5;
	color:#fff;
}

.WdateDiv .Wwday{
	cursor:pointer;
	color:#ff6600;
}

.WdateDiv .WwdayOn{
	cursor:pointer;
	color:#fff;
	background-color:#6cbcf5;
}
.WdateDiv .Wtoday{
	cursor:pointer;
	color:#f60;
}
.WdateDiv .Wselday{
	background-color:#1797dc;
	color:#fff;
}
.WdateDiv .WspecialDay{
	background-color:#66F4DF;
}

.WdateDiv .WotherDay{ 
	cursor:pointer;
	color:#6cbcf5;	
}

.WdateDiv .WotherDayOn{ 
	cursor:pointer;
	background-color:#6cbcf5;	
	color:#fff;
}

.WdateDiv .WinvalidDay{
	color:#aaa;
}

.WdateDiv #dpTime{
	float:left;
	margin-top:8px;
	margin-left:15px;
	width:80%;
}

.WdateDiv #dpTime #dpTimeStr{
	margin-left:1px;
}
.WdateDiv #dpTime input{
	width:24px;
	height:20px;
	text-align:center;
	border:#d5d3d4 1px solid;
}

.WdateDiv #dpTime .tB{
	border-right:0px;
}

.WdateDiv #dpTime .tE{
	border-left:0;
	border-right:0;
}

.WdateDiv #dpTime .tm{
	width:7px;
	border-left:0;
	border-right:0;
}

.WdateDiv #dpTime #dpTimeUp{
	height:10px;
	width:13px;
	border:0px;
	background:url(img.gif) no-repeat -32px -16px;
}

.WdateDiv #dpTime #dpTimeDown{
	height:10px;
	width:13px;
	border:0px;
    background:url(img.gif) no-repeat -48px -16px;
}

 .WdateDiv #dpQS {
 	float:left;
	margin-right:3px;
	margin-top:3px;
	background:url(img.gif) no-repeat 0px -16px;
	width:20px;
	height:20px;
	cursor:pointer;
 }
.WdateDiv #dpControl {
	text-align:right;	
	height:auto;
}   

.WdateDiv .dpButton{ 
	height:28px;
	width:72px;
	border:0;
	border-radius:3px;
	background:#6cbcf5;
	color:#fff;
	margin:14px 10px 8px 0px;
	cursor:pointer;
}
#dpClearInput{
  background:#fff;
  color:#6cbcf5;
}
#dpOkInput:hover{background:#1797dc;}
#dpClearInput:hover{background:#6cbcf5;color:#fff;}